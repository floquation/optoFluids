FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    INLET
    {
        type	zeroGradient;
    }
    WALL
    {
        type	zeroGradient;
    }
    OUT1
    {
        type	zeroGradient;     
    }
    OUT2
    {
	type	zeroGradient;
    }
}


