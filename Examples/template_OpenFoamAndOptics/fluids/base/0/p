FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{

    walls
    {
        type            zeroGradient;
    }


    cycLeft
    {      
        type            fan;
        patchType       cyclic;
        jumpTable polynomial 1((2.6780625E-4 0)); // Pressure drop: DeltaP/rho
        // The list is a list of polynomial coefficients: f_1 + f_2*v + f_3*v^2 + ...
        value           uniform 0;
    }                                                                                

    cycRight
    {       
        type            fan;
        patchType       cyclic;
    }

